# Database (required)
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Session (required)
SESSION_SECRET=your-session-secret-here

# Replit Auth (auto-set on Replit, required for local dev)
REPL_ID=your-repl-id
REPL_SLUG=your-repl-slug
REPL_OWNER=your-replit-username
REPLIT_DEV_DOMAIN=your-repl-slug.your-username.repl.co
ISSUER_URL=https://replit.com/oidc

# Google Gemini AI (required)
AI_INTEGRATIONS_GEMINI_API_KEY=your-gemini-api-key
AI_INTEGRATIONS_GEMINI_BASE_URL=https://generativelanguage.googleapis.com

# Telegram Bot (required)
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHAT_ID=your-admin-telegram-chat-id

# Reddit Posting (optional - enables auto-posting replies to Reddit)
REDDIT_CLIENT_ID=your-reddit-client-id
REDDIT_CLIENT_SECRET=your-reddit-client-secret
REDDIT_USERNAME=your-reddit-username
REDDIT_PASSWORD=your-reddit-password

